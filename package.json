{
  "name": "storymap",
  "version": "1.20.0",
  "type": "module",
  "description": "A small tool for understanding the structure of interlinked markdown files.",
  "bin": "index.js",
  "keywords": [
    "graphviz",
    "markdown"
  ],
  "files": [
    "*.js",
    "README.md"
  ],
  "main": "index.js",
  "author": "Aria Stewart <aredridel@dinhe.net>",
  "license": "ISC",
  "dependencies": {
    "@aredridel/url-relative": "^1.1.0",
    "array-uniq": "^2.1.0",
    "compromise": "^11.14.3",
    "front-matter": "^3.0.2",
    "remark-parse": "^7.0.2",
    "remark-retext": "^3.1.3",
    "retext-english": "^3.0.4",
    "retext-pos": "^2.0.2",
    "retext-stringify": "^2.0.4",
    "unified": "^8.4.2",
    "word-wrap": "^1.2.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">13.0.0"
  }
}
